;; deploy_WithoutDeploy.storage.fc
;;
;; Type: WithoutDeploy
;; TLB: _  = WithoutDeploy
;;

tuple $WithoutDeploy$_contract_load() impure inline {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return null();
    } else {
        return $WithoutDeploy$_contract_init();
    }
}

;;
;; Contract WithoutDeploy functions
;;

tuple $WithoutDeploy$_contract_init() impure inline {
    tuple $self = null();
    return $self;
}

;;
