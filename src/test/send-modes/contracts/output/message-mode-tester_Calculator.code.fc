;;

((int, cell, int, slice, int)) $MessageParameters$_constructor_to_bounce_value_body(slice $to, int $bounce, int $value, cell $body) inline {
    return (0, $body, $value, $to, $bounce);
}

;;
;; Type: AverageRequest
;; Header: 0xc6efd40e
;; TLB: average_request#c6efd40e from:uint32 to:uint32 amountToPayInResponse:int257 = AverageRequest
;;

(slice, ((int, int, int))) $AverageRequest$_load_without_opcode(slice sc_0) inline {
    var v'from = sc_0~load_uint(32);
    var v'to = sc_0~load_uint(32);
    var v'amountToPayInResponse = sc_0~load_int(257);
    return (sc_0, (v'from, v'to, v'amountToPayInResponse));
}

;;
;; Type: AverageResult
;; Header: 0x016bce84
;; TLB: average_result#016bce84 res:uint32 = AverageResult
;;

builder $AverageResult$_store(builder build_0, (int) v) inline {
    var (v'res) = v;
    build_0 = store_uint(build_0, 23842436, 32);
    build_0 = build_0.store_uint(v'res, 32);
    return build_0;
}

cell $AverageResult$_store_cell((int) v) inline {
    return $AverageResult$_store(begin_cell(), v).end_cell();
}

((int)) $AverageResult$_constructor_res(int $res) inline {
    return ($res);
}

