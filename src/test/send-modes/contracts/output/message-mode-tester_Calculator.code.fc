;;
;; Routing of a Contract Calculator
;;

() recv_internal(int msg_value, cell in_msg_cell, slice in_msg) impure {
    
    ;; Context
    var cs = in_msg_cell.begin_parse();
    cs~skip_bits(2);
    var msg_bounceable = cs~load_int(1);
    var msg_bounced = cs~load_int(1);
    slice msg_sender_addr = cs~load_msg_addr();
    __tact_context = (msg_bounceable, msg_sender_addr, msg_value, cs);
    __tact_context_sender = msg_sender_addr;
    
    ;; Load contract data
    var $self = $Calculator$_contract_load();
    
    ;; Handle bounced messages
    if (msg_bounced) { return (); }
    int op = 0;
    int in_msg_length = slice_bits(in_msg);
    if (in_msg_length >= 32) {
        op = in_msg~load_uint(32);
    }
    ;; Receive AverageRequest message
    if (op == 3337606158) {
        var ($request'from, $request'to, $request'amountToPayInResponse) = in_msg~$AverageRequest$_load_without_opcode();
        throw_unless(25459, (($request'to - $request'from) >= 0));
        int $n = (($request'to - $request'from) + 1);
        int $count = $request'from;
        int $sum = 0;
        while (($count <= $request'to)) {
            $sum = $sum + $count;
            $count = $count + 1;
        }
        int $result = ($sum / $n);
        $global_send($SendParameters$_constructor_to_bounce_value_body(__tact_context_get_sender(), false, $request'amountToPayInResponse, $AverageResult$_store_cell($AverageResult$_constructor_res($result))));
        ;; Persist state
        $Calculator$_contract_store($self);
        return ();
    }
    
    ;; Receive empty message
    if ((op == 0) & (in_msg_length <= 32)) {
        ;; Persist state
        $Calculator$_contract_store($self);
        return ();
    }
    ;; Throw if not handled
    throw(130);
}
