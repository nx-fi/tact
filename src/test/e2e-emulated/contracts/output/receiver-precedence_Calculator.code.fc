            $global_send($SendParameters$_constructor_to_bounce_value_body(__tact_context_get_sender(), false, 1000000000, $BinaryIntResult$_store_cell($BinaryIntResult$_constructor_val($result))));
            ;; Persist state
            $Calculator$_contract_store($self);
            return ();
        }
        
    }
    ;; Empty Receiver and Text Receivers
    var text_op = slice_hash(in_msg);
    ;; Receive "deploy" message
    if (text_op == 0xd95abec808516815126b60e61ee97798fa369d80938fd6e58a862d5789af459c) {
        ;; Persist state
        $Calculator$_contract_store($self);
        return ();
    }
    ;; Throw if not handled
    throw(130);
}
