(slice) $C$_contract_load() impure inline {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return $sc~$C$_load();
    } else {
        (slice $parent) = $sc~$C$init$_load();
        $sc.end_parse();
        return $C$_contract_init($parent);
    }
}

