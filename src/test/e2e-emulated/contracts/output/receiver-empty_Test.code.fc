#pragma compute-asm-ltr;

;; receiver-empty_Test.headers.fc
;;
;; Header files for Test
;; NOTE: declarations are sorted for optimal order
;;

;; $Test$_contract_init
tuple $Test$_contract_init() impure inline;

;; $Test$_contract_load
tuple $Test$_contract_load() impure inline_ref;

;; $Test$_contract_store
() $Test$_contract_store(tuple v) impure inline;

;; $Test$_fun_balance
(tuple, int) $Test$_fun_balance(tuple $self) impure inline_ref;


;; receiver-empty_Test.stdlib.fc
global (int, slice, int, slice) __tact_context;
global slice __tact_context_sender;
global cell __tact_child_contract_codes;
global int __tact_randomized;

int $global_myBalance() impure asm """
    BALANCE FIRST
""";

;; receiver-empty_Test.storage.fc
;;
;; Type: Test
;;

tuple $Test$_contract_load() impure inline_ref {
    slice $sc = get_data().begin_parse();
    int $loaded = $sc~load_int(1);
    if ($loaded) {
        return null();
    } else {
        return $Test$_contract_init();
    }
}

() $Test$_contract_store(tuple v) impure inline {
    builder b = begin_cell();
    b = b.store_int(true, 1);
    set_data(b.end_cell());
}

;;
;; Contract Test functions
;;

tuple $Test$_contract_init() impure inline {
    tuple $self = null();
    return $self;
}

(tuple, int) $Test$_fun_balance(tuple $self) impure inline_ref {
    var ($self) = $self;
    var $fresh$ret_677 = $global_myBalance();
    return ($self, $fresh$ret_677);
}

;;
;; Get methods of a Contract Test
;;

_ %balance() method_id(104128) {
    var self = $Test$_contract_load();
    var res = self~$Test$_fun_balance();
    return res;
}

;;
;; Routing of a Contract Test
;;

() recv_internal(int msg_value, cell in_msg_cell, slice in_msg) impure {
    
    ;; Context
    var cs = in_msg_cell.begin_parse();
    cs~skip_bits(2);
    var msg_bounceable = cs~load_int(1);
    var msg_bounced = cs~load_int(1);
    slice msg_sender_addr = cs~load_msg_addr();
    __tact_context = (msg_bounceable, msg_sender_addr, msg_value, cs);
    __tact_context_sender = msg_sender_addr;
    
    ;; Load contract data
    var $self = $Test$_contract_load();
    
    ;; Handle bounced messages
    if (msg_bounced) { return (); }
    int op = 0;
    int in_msg_length = slice_bits(in_msg);
    if (in_msg_length >= 32) {
        op = in_msg~load_uint(32);
    }
    ;; Receive empty message
    if ((op == 0) & (in_msg_length <= 32)) {
        ;; Persist state
        $Test$_contract_store($self);
        return ();
    }
    ;; Throw if not handled
    throw(130);
}
